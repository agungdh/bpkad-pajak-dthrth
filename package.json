{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "format": "npx concurrently \"npm:format:*\"",
        "format:js": "prettier --write \"resources/**/*.{js,jsx,ts,tsx,css,blade.php}\"",
        "format:php": "vendor/bin/pint",
        "lint": "npx concurrently \"npm:lint:*\"",
        "lint:js": "prettier --check \"resources/**/*.{js,jsx,ts,tsx,css,blade.php}\"",
        "lint:php": "vendor/bin/pint --test"
    },
    "dependencies": {
        "@fontsource/source-sans-3": "^5.2.9",
        "@popperjs/core": "^2.11.8",
        "@tailwindcss/vite": "^4.1.11",
        "admin-lte": "^4.0.0-rc6",
        "alpinejs": "^3.15.3",
        "autoprefixer": "^10.4.20",
        "axios": "^1.13.2",
        "bootstrap": "^5.3.8",
        "bootstrap-icons": "^1.13.1",
        "concurrently": "^9.0.1",
        "datatables.net": "^2.3.6",
        "datatables.net-bs5": "^2.3.6",
        "datatables.net-select-bs5": "^3.1.3",
        "jquery": "^3.7.1",
        "laravel-vite-plugin": "^2.0",
        "overlayscrollbars": "^2.13.0",
        "select2": "^4.0.13",
        "select2-bootstrap-5-theme": "^1.3.0",
        "sweetalert2": "^11.26.17",
        "tailwindcss": "^4.0.7",
        "toastr": "^2.1.4",
        "vite": "^7.0.4"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5",
        "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
        "lightningcss-linux-x64-gnu": "^1.29.1"
    },
    "devDependencies": {
        "prettier": "^3.7.4",
        "prettier-plugin-blade": "^2.1.21",
        "prettier-plugin-tailwindcss": "^0.7.2"
    }
}
